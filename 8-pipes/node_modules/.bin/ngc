else
  export NODE_PATH="/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8.2_zone_wmbye3b6f5zz34qd7k6bsf54ue/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8.2_zone_wmbye3b6f5zz34qd7k6bsf54ue/node_modules/@angular/compiler-cli/bundles/src/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8.2_zone_wmbye3b6f5zz34qd7k6bsf54ue/node_modules/@angular/compiler-cli/bundles/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8.2_zone_wmbye3b6f5zz34qd7k6bsf54ue/node_modules/@angular/compiler-cli/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8.2_zone_wmbye3b6f5zz34qd7k6bsf54ue/node_modules/@angular/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/@angular+compiler-cli@18.2.13_@angular+compiler@18.2.13_@angular+core@18.2.13_rxjs@7.8.2_zone_wmbye3b6f5zz34qd7k6bsf54ue/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/node_modules:$NODE_PATH"
