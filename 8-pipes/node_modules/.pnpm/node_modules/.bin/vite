#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.14_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules/vite/bin/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.14_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules/vite/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.14_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.14_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules/vite/bin/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.14_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules/vite/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.14_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules:/home/wilson/angular-practices/8-pipes/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
